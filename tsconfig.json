{
  "compilerOptions": {
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "target": "es2015",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "watch": true,
    "outDir": "build_ES2015_CommonJS/"
  },
  "include": [
    "ts/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
